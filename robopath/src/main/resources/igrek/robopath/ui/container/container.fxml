<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.HBox?>
<HBox prefWidth="800" prefHeight="600" HBox.hgrow="ALWAYS" xmlns="http://javafx.com/javafx/8.0.121"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="igrek.robopath.ui.container.ContainerController">
    <children>
        <HBox HBox.hgrow="ALWAYS">
            <children>
                <TabPane fx:id="containerTabPane" HBox.hgrow="ALWAYS">
                    <tabs>
                        <Tab text="Simple A*">
                            <content>
                                <fx:include source="../simpleastar/simpleastar.fxml"/>
                            </content>
                        </Tab>
                        <Tab text="A* test">
                            <content>
                                <fx:include source="../robomove/robomove.fxml"/>
                            </content>
                        </Tab>
                    </tabs>
                </TabPane>
            </children>
        </HBox>
    </children>
</HBox>
